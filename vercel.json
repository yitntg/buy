{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next",
      "config": {
        "installCommand": "npm install critters && npm install"
      }
    }
  ],
  "env": {
    "DISABLE_CRITTERS": "true",
    "NEXT_FORCE_DYNAMIC": "1"
  },
  "routes": [
    {
      "src": "/admin/(.*)",
      "headers": {
        "Cache-Control": "no-store, max-age=0"
      }
    }
  ]
} 